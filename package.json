{
  "name": "@indentapis/indent-js",
  "version": "0.2.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/jest": "^24.0.11",
    "@types/mocha": "^5.2.0",
    "@types/node": "^13.7.0",
    "@types/node-fetch": "^2.5.4",
    "@types/raven": "^2.5.1",
    "@types/sinon": "^7.0.11",
    "jest": "^25.1.0",
    "mocha": "^6.1.4",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.17.0",
    "prettier-check": "^2.0.0",
    "replace-in-file": "^4.0.0",
    "rimraf": "^2.6.3",
    "sinon": "^7.3.2",
    "ts-jest": "^25.1.0",
    "tslib": "^1.10.0",
    "tslint": "^5.16.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.4.5",
    "typescript-tslint-plugin": "^0.3.1"
  },
  "dependencies": {
    "@yarnpkg/pnpify": "^2.0.0-rc.17"
  },
  "scripts": {
    "build": "yarn workspaces foreach -pt --verbose run build",
    "publish": "yarn workspaces foreach -pt --exclude @indentapis/indent-js npm publish",
    "test": "yarn workspaces foreach -pt run test",
    "bump-patch": "yarn workspaces foreach -t version patch -i",
    "bump-minor": "yarn workspaces foreach -t version minor -i",
    "bump-major": "yarn workspaces foreach -t version major -i"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "yarn@3.0.2"
}
